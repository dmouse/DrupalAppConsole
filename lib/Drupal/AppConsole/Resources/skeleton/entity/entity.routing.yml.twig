{{entity_name}}.view:
  path: '/{{entity_name}}/{{ '{' ~ entity_name ~ '}' }}'
  defaults:
    _entity_view: '{{entity_name}}'
    _title: '{{entity_label}} Content'
  requirements:
    _permission: 'view_{{entity_name}}'
{{entity_name}}.list:
  path: '/{{entity_name}}/list'
  defaults:
    _entity_list: '{{entity_name}}'
    _title: '{{entity_label}} Content List'
  requirements:
    _permission: 'view_{{entity_name}}'
{{entity_name}}.add:
  path: '/{{entity_name}}/add'
  defaults:
    _entity_form: {{entity_name}}.add
    _title: 'Add {{entity_label}} Content'
  requirements:
    _permission: 'add_{{entity_name}}'
{{entity_name}}.edit:
  path: '/{{entity_name}}/{{ '{' ~ entity_name~ '}' }}/edit'
  defaults:
    _entity_form: {{entity_name}}.edit
    _title: 'Edit {{entity_label}} content'
  requirements:
    _permission: 'edit_{{entity_name}}'
{{entity_name}}.delete:
  path: '/{{entity_name}}/{{ '{' ~ entity_name~ '}'}}/delete'
  defaults:
    _entity_form: {{entity_name}}.delete
    _title: 'Delete {{entity_label}} Content '
  requirements:
    _permission: 'delete_{{entity_name}}'
